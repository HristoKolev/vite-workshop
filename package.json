    "jsdom": "^24.1.0",
    "msw": "^2.3.1",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "sass": "^1.77.6",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.4",
    "typescript": "~5.4.5",
    "typescript-eslint": "^7.13.1",
